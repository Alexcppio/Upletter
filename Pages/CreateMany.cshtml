@page "/pages/createmany"
@model CreateManyModel

<div class="m-2">
    <h4 class="bg-primary text-white text-center p-2">Слова, которые нужно добавить</h4>
    <table class="table table-sm table-bordered table-striped">
        <thead>
            <tr>
                <th>№</th>
                <th>Имя</th>
            </tr>
        </thead>
        <tbody>
            <div class="row">
                <div class="col">
                    <h5 class="bg-primary text-white text-center m-2 p-2">Список найденных слов</h5>
                    <ul class="list-group m-2">
                            <li class="list-group-item">
                                <div class="form-check">
                                    <input type="checkbox" class="form-check-input" id="exampleCheck1" checked>
                                    <label class="form-check-label" for="exampleCheck1">@TempData["Head"]</label>
                                </div>
                            </li>
                    </ul>
                </div>
            </div>
        </tbody>
    </table>
    <a asp-page="Create" class="btn btn-primary">Добавить</a>
    <a asp-page="StartPage" class="btn btn-primary">Назад</a>
</div>


